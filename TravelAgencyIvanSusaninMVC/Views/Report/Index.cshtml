@model IEnumerable<TravelAgencyIvanSusaninDAL.ViewModel.TravelsReservationsViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TravelId)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TravelId)
        </td>
    </tr>   
        foreach (var recerv in item.Reservations)
        {
         <tr>
            <td>
                @Html.DisplayFor(modelItem => recerv.Item1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => recerv.Item2)
            </td>
          </tr>
        }
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
        </tr>
           
     }

</table>

